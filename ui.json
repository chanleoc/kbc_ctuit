{
    "title": "Ctuit Extractor",
    "type": "object",
    "required": [
        "X-UserID",
        "#X-UserAuthToken",
        "template",
        "active_restaurant_loop",
        "start_date",
        "end_date",
        "custom_payload"

    ],
    "properties": {
        "X-UserID": {
            "type": "string",
            "title": "User ID",
            "description": "User APi ID",
            "propertyOrder": 1
        },
        "#X-UserAuthToken":{
            "type": "string",
            "title": "User Auth Token",
            "description": "User APi Token",
            "propertyOrder": 2
        },
        "template": {
            "type": "string",
            "title": "Endpoint Template",
            "enum": [
                "dine-in",
                "catering",
                "labor",
                "flash",
                "waste",
                "ingredient-orders",
                "custom"
            ],
            "propertyOrder": 3
        },
        "active_restaurant_loop":{
            "type": "boolean",
            "title": "Active Loop",
            "description": "Looping thru the active restaurant location_ids instead of the location_group_id",
            "default": false,
            "propertyOrder": 4
        },
        "start_date":{
            "type": "string",
            "title": "Start Date",
            "description": "Default: today; Alternate Parameters: 1 day ago, 2 days ago, 1 week ago",
            "default": "today",
            "propertyOrder": 5
        },
        "end_date": {
            "type": "string",
            "title": "End Date",
            "description": "Default: today; Alternate Parameters: 1 day ago, 2 days ago, 1 week ago",
            "default": "today",
            "propertyOrder": 6
        },
        "custom_payload": {
            "type": "string",
            "title": "Custom Payload",
            "description": "Have to select custom template",
            "default": {},
            "format": "json",
            "propertyOrder": 7
        }
    }
}